<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.livroandroid.cap9.activityanimation_googledemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnimatedSubActivity"
            android:parentActivityName="br.com.livroandroid.cap9.activityanimation_googledemo.MainActivity"
            android:label="@string/title_activity_window_anim_sub" />
        <activity
            android:name="br.com.livroandroid.cap9.activityanimation_googledemo.SubActivity"
            android:parentActivityName="br.com.livroandroid.cap9.activityanimation_googledemo.MainActivity"
            android:label="@string/title_activity_sub" />
    </application>

</manifest>