<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.livroandroid.cap6.layout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExemploLinearLayoutAPIActivity"
            android:label="LinearLayout"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ExemploTableLayoutAPIActivity"
            android:label="TableLayout"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ExemploScrollViewActivity"
            android:label="ScrollView"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>